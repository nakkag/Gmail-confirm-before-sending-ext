{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.1",
  "description": "__MSG_extDescription__",
  "permissions": ["storage"],
  "host_permissions": ["https://mail.google.com/*"],
  "default_locale": "en",
  "options_page": "option.html",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
